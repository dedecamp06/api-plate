{
  "name": "api-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "cls; set NODE_ENV=dev&& nodemon src/new/server.ts",
    "start:old": "node dist/infra/web/server.js",
    "dev": "set NODE_ENV=dev&& nodemon src/infra/web/server.ts",
    "prod": "nodemon src/infra/web/server.ts",
    "dev:server": "ts-node-dev -r tsconfig-paths/register --transpile-only --ignore-watch node_modules src/infra/web/server.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist  --copy-files",
    "tsc": "tsc",
    "test": "npm run tsc -- --noEmit && jest",
    "swagger-autogen": "node swagger.js",
    "sequelize:service:migrate": "npx sequelize-cli@6.2.0 --options-path ./.sequelize-service db:migrate"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@swc/jest": "^0.2.20",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.39",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^27.5.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.172",
    "@types/swagger-ui-express": "^4.1.3",
    "config": "^3.3.7",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "nodemon": "^2.0.16",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.3.5",
    "sequelize-typescript": "^2.1.3",
    "swagger-autogen": "^2.21.1",
    "swagger-ui-express": "^4.3.0",
    "tedious": "^14.5.0",
    "ts-jest": "^27.1.4",
    "tsyringe": "^4.6.0"
  }
}
